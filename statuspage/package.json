{
  "name": "statuspage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "codegen-watch": "graphql-codegen -r dotenv/config --config codegen.ts dotenv_config_path=.env.local --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@graphql-codegen/plugin-helpers": "^5.1.1",
    "@hpcc-js/wasm": "^2.25.0",
    "@mdi/font": "^7.4.47",
    "@vue/apollo-composable": "^4.2.2",
    "apexcharts": "^5.3.2",
    "aws-appsync-auth-link": "^3.0.7",
    "aws-appsync-subscription-link": "^3.1.3",
    "buffer": "^6.0.3",
    "graphql": "^16.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue3-apexcharts": "^1.8.0",
    "vuetify": "^3.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nabla/vite-plugin-eslint": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "esbuild": "0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "prettier": "^3.0.0",
    "typescript": "^5.5.0",
    "vite": "^7.0.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.3"
  }
}
